{
  "name": "b-connect/bc_total_control",
  "description": "b-connect Total Control Dashboard: A dashboard is what is missing for better Drupal administration experience. This dashboard is built on top of Total Control Admin Dashboard module, utilizing Panels, Google Analytics Reports, and several enhanced blocks and widgets for an intuitive and flexible administration experience.",
  "type": "drupal-module",
  "homepage": "https://b-connect.de/",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "b-connect GmbH",
      "homepage": "https://b-connect.de/",
      "role": "Maintainer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
      },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "~1.0",
    "oomphinc/composer-installers-extender": "~1.0",
    "cweagans/composer-patches": "~1.0",
    "drupal/drupal-library-installer-plugin": "^0.3",
    "drupal/masonry": "~1.0",
    "drupal/charts": "~3.0",
    "drupal/google_analytics_reports": "~3.0",
    "drupal/total_control": "~2.0",
    "drupal/views_bootstrap": "~4.0",
    "npm-asset/highcharts": "^7.0"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-6.x": "8.6.x-dev"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patches": {
        "drupal/total_control": {
            "Issue #2935351: Fixed the issue when Enabled with multilingual of Translatable Markup for Invalid Argument Exception: $string (Array) must be a string.":
            "https://www.drupal.org/files/issues/2935351-2.patch"
        },
        "drupal/google_analytics_reports": {
            "Removed the MAINTNANCE_MODE check":
	    "https://gist.githubusercontent.com/merlin-tribukait/85b0f4d62fcc405fa19cddb10d70db0d/raw/598b73f7ff42c0151efb04d63f7bb9242bf2bf0e/maintenance_mode_fix.patch"
        }
    }
  }
}
